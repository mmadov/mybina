<p id="notice"><%= notice %></p>

<h1>Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>About</th>
      <th>Slide</th>
      <th>Logo</th>
      <th>Image</th>
      <th>Adress</th>
      <th>User</th>
      <th>Phone</th>
      <th>Facebook</th>
      <th>Youtube</th>
      <th>Web site</th>
      <th>Floor</th>
      <th>Part</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.about %></td>
        <td><%= company.slide %></td>
        <td><%= company.logo %></td>
        <td><%= company.image %></td>
        <td><%= company.adress %></td>
        <td><%= company.user_id %></td>
        <td><%= company.phone %></td>
        <td><%= company.facebook %></td>
        <td><%= company.youtube %></td>
        <td><%= company.web_site %></td>
        <td><%= company.floor %></td>
        <td><%= company.part %></td>
        <td><%= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
